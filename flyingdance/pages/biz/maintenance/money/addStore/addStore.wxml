<form bindsubmit='sub'>
<view class="page" xmlns:wx="http://www.w3.org/1999/xhtml">
  <view class="page__bd">
    <view class="weui-cells__title">添加库存</view>
    <view class="weui-cells weui-cells_after-title">
      <view class="weui-cell weui-cell_input">
        <view class="weui-cell__hd">
          <view class="weui-label" style="padding:10rpx">品名</view>
        </view>
        <view class="weui-cell__bd">
          <input class="weui-input" name='sproduct_Name' placeholder="请输入品名" />
        </view>
      </view>
      <view class="weui-cell weui-cell_input">
        <view class="weui-cell__hd">
          <view class="weui-label" style="padding:10rpx">品牌</view>
        </view>
       <view class="weui-cell__bd">
          <input class="weui-input" name='sbrand' placeholder="请输入品牌" />
        </view>
      </view>
      <view class="weui-cell weui-cell_input">
        <view class="weui-cell__hd">
          <view class="weui-label" style="padding:10rpx">规格</view>
        </view>
        <view class="weui-cell__bd">
          <input class="weui-input" name='specifications' placeholder="请输入规格" />
        </view>
      </view>
      <view class="graySpace"></view>
      <view class="weui-cell weui-cell_input">
        <view class="weui-cell__hd">
          <view class="weui-label" style="padding:10rpx">类别</view>
        </view>
        <view class="weui-cell__bd">
          <input class="weui-input" name='scategory' placeholder="请输入类别" />
        </view>
      </view>
      <view class="weui-cell weui-cell_input">
        <view class="weui-cell__hd">
          <view class="weui-label" style="padding:10rpx">采购价格</view>
        </view>
        <view class="weui-cell__bd">
          <input class="weui-input" name='sbuy_price' placeholder="请输入采购价格" />
        </view>
      </view>
      <view class="graySpace"></view>
      <view class="weui-cell weui-cell_input">
        <view class="weui-cell__hd">
          <view class="weui-label" style="padding:10rpx">指导价格</view>
        </view>
        <view class="weui-cell__bd">
          <input class="weui-input" name='sguide_price' placeholder="请输入指导价格" />
        </view>
    
      </view>
      <view class="graySpace"></view>
       <view class="weui-cell weui-cell_input">
        <view class="weui-cell__hd">
          <view class="weui-label" style="padding:10rpx">计量单位</view>
        </view>
         <view class="weui-cell__bd">
          <input class="weui-input" name='sunit' placeholder="请输入计量单位" />
        </view>
     </view>
     <view class="graySpace"></view>
     <view class="weui-cell weui-cell_input">
        <view class="weui-cell__hd">
          <view class="weui-label" style="padding:10rpx">所属部门</view>
        </view>
        <view class="weui-cell__bd">
          <input class="weui-input" name='school' placeholder="请输入所属部门" />
        </view>
      </view>
      <view class="weui-cell weui-cell_input">
        <view class="weui-cell__hd">
          <view class="weui-label" style="padding:10rpx">库存数量</view>
        </view>
        <view class="weui-cell__bd">
          <input class="weui-input" name='snumber' placeholder="请输入库存数量" />
        </view>
      </view>
   <view class="weui-cell weui-cell_input">
        <view class="weui-cell__hd">
          <view class="weui-label" style="padding:10rpx">创建日期</view>
        </view>
        <view class="weui-cell__bd">
          <picker mode="date" value="{{time}}" name="time" start="2010-01-01" end="2030-01-01" bindchange="bindtimeChange">
            <view class="picker" name="time">
              选择: {{time}}
            </view>
          </picker>
        </view>
      </view>








       </view>
    <view class="weui-btn-area">

      <button class="weui-btn" type="primary" formType="submit">确定</button>

    </view>
 

</view>
</view>
</form>